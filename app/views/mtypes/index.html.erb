<div class="content">
<h1>Material Types - mtypes#index</h1>
<%= link_to 'New Material Type', new_mtype_path %>
<table>
  <tr>
    <th>Code</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mtypes.each do |mtype| %>
  <tr>
    <td><%= mtype.code %></td>
    <td><%= mtype.desc %></td>
    <td><%= link_to 'Show', mtype %></td>
    <td><%= link_to 'Edit', edit_mtype_path(mtype) %></td>
    <td><%= link_to 'Destroy', mtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
</div>